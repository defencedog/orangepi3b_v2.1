setup_write_uboot_platform()
{

        if grep -q "ubootpart" /proc/cmdline; then

                local tmp=3D$(cat /proc/cmdline)
                tmp=3D"${tmp##*ubootpart=3D}"
                tmp=3D"${tmp%% *}"
                [[ -n $tmp ]] &amp;&amp; local part=3D$(findfs PARTUUID=3D$=
tmp 2&gt;/dev/null)
                [[ -n $part ]] &amp;&amp; local dev=3D$(lsblk -n -o PKNAME =
$part 2&gt;/dev/null)
                [[ -n $dev ]] &amp;&amp; DEVICE=3D"/dev/$dev"

        fi
}

