ion}.deb" &gt;&gt; "${DEST}"/${LOG_SUBPATH}/output.log 2&gt;&amp;1
        mkdir -p "${DEB_STORAGE}/${RELEASE}/"
        rsync --remove-source-files -rq "${destination}.deb" "${DEB_STORAGE=
}/${RELEASE}/"

        # cleanup
        rm -rf ${bsptempdir}
}
