                # TODO: Test which output driver is better with DRM
                mkdir -p "${destination}"/etc/mpv/
                cp "${EXTER}"/packages/bsp/mpv/mpv_mainline.conf "${destina=
tion}"/etc/mpv/mpv.conf
        fi

        case $RELEASE in
