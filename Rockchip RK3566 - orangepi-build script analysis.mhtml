<pre class=3D"highlighter-hljs" highlighted=3D"true"><code class=3D"languag=
e-shell highlighter-hljs hljs">install_common()
{
        display_alert "Applying common tweaks" "" "info"

        # install rootfs encryption related packages separate to not break =
packages cache
        if [[ $CRYPTROOT_ENABLE =3D=3D yes ]]; then
                display_alert "Installing rootfs encryption related package=
s" "cryptsetup" "info"
                chroot "${SDCARD}" /bin/bash -c "apt-get -y -qq --no-instal=
l-recommends install cryptsetup" \
                &gt;&gt; "${DEST}"/${LOG_SUBPATH}/install.log 2&gt;&amp;1
                if [[ $CRYPTROOT_SSH_UNLOCK =3D=3D yes ]]; then
                        display_alert "Installing rootfs encryption related=
 packages" "dropbear-initramfs" "info"
