<pre class=3D"highlighter-hljs" highlighted=3D"true"><code class=3D"languag=
e-shell highlighter-hljs hljs">compile_kernel()
{
        if [[ $CLEAN_LEVEL =3D=3D *make* ]]; then
                display_alert "Cleaning" "$LINUXSOURCEDIR" "info"
                (cd ${LINUXSOURCEDIR}; make ARCH=3D"${ARCHITECTURE}" clean =
&gt;/dev/null 2&gt;&amp;1)
        fi

        if [[ $USE_OVERLAYFS =3D=3D yes ]]; then
                local kerneldir
                kerneldir=3D$(overlayfs_wrapper "wrap" "$LINUXSOURCEDIR" "k=
ernel_${LINUXFAMILY}_${BRANCH}")
        else
                local kerneldir=3D"$LINUXSOURCEDIR"
        fi
        cd "${kerneldir}" || exit

        rm -f localversion

        # read kernel version
        local version hash
        version=3D$(grab_version "$kerneldir")

        # read kernel git hash
        hash=3D$(improved_git --git-dir=3D"$kerneldir"/.git rev-parse HEAD)

        # Apply a series of patches if a series file exists
        if test -f "${EXTER}"/patch/kernel/${KERNELPATCHDIR}/series.conf; t=
hen
                display_alert "series.conf file visible. Apply"
                series_conf=3D"${SRC}"/patch/kernel/${KERNELPATCHDIR}/serie=
s.conf

                # apply_patch_series &lt;target dir&gt; &lt;full path to se=
ries file&gt;
                apply_patch_series "${kerneldir}" "$series_conf"
        fi

        # build 3rd party drivers
        # compilation_prepare

        advanced_patch "kernel" "$KERNELPATCHDIR" "$BOARD" "" "$BRANCH" "$L=
INUXFAMILY-$BRANCH"

        # create patch for manual source changes in debug mode
        [[ $CREATE_PATCHES =3D=3D yes ]] &amp;&amp; userpatch_create "kerne=
l"

        # re-read kernel version after patching
        local version
        version=3D$(grab_version "$kerneldir")

        display_alert "Compiling $BRANCH kernel" "$version" "info"

        # compare with the architecture of the current Debian node
        # if it matches we use the system compiler
        if $(dpkg-architecture -e "${ARCH}"); then
                display_alert "Native compilation"
        elif [[ $(dpkg --print-architecture) =3D=3D amd64 ]]; then
                local toolchain
                toolchain=3D$(find_toolchain "$KERNEL_COMPILER" "$KERNEL_US=
E_GCC")
                [[ -z $toolchain ]] &amp;&amp; exit_with_error "Could not f=
ind required toolchain" "${KERNEL_COMPILER}gcc $KERNEL_USE_GCC"
        else
                exit_with_error "Architecture [$ARCH] is not supported"
        fi

        display_alert "Compiler version" "${KERNEL_COMPILER}gcc $(eval env =
PATH=3D"${toolchain}:${PATH}" "${KERNEL_COMPILER}gcc" -dumpversion)" "info"

        # copy kernel config
        if [[ $KERNEL_KEEP_CONFIG =3D=3D yes &amp;&amp; -f "${DEST}"/config=
/$LINUXCONFIG.config ]]; then
                display_alert "Using previous kernel config" "${DEST}/confi=
g/$LINUXCONFIG.config" "info"
                cp -p "${DEST}/config/${LINUXCONFIG}.config" .config
        else
                if [[ -f $USERPATCHES_PATH/$LINUXCONFIG.config ]]; then
                        display_alert "Using kernel config provided by user=
" "userpatches/$LINUXCONFIG.config" "info"
                        cp -p "${USERPATCHES_PATH}/${LINUXCONFIG}.config" .=
config
                else
                        display_alert "Using kernel config file" "${EXTER}/=
config/kernel/$LINUXCONFIG.config" "info"
                        cp -p "${EXTER}/config/kernel/${LINUXCONFIG}.config=
" .config
                fi
        fi

        call_extension_method "custom_kernel_config" &lt;&lt; 'CUSTOM_KERNE=
L_CONFIG'
*Kernel .config is in place, still clean from git version*
Called after ${LINUXCONFIG}.config is put in place (.config).
Before any olddefconfig any Kconfig make is called.
A good place to customize the .config directly.
CUSTOM_KERNEL_CONFIG


        # hack for deb builder. To pack what's missing in headers pack.
        cp "$EXTER"/patch/misc/headers-debian-byteshift.patch /tmp

        if [[ $KERNEL_CONFIGURE !=3D yes ]]; then
                if [[ $BRANCH =3D=3D legacy &amp;&amp; ! $BOARDFAMILY =3D~ =
"rockchip-rk3588"|"rockchip-rk356x" ]]; then
                        eval CCACHE_BASEDIR=3D"$(pwd)" env PATH=3D"${toolch=
ain}:${PATH}" \
                                'make ARCH=3D$ARCHITECTURE CROSS_COMPILE=3D=
"$CCACHE $KERNEL_COMPILER" silentoldconfig'
                else
                        # TODO: check if required
                        eval CCACHE_BASEDIR=3D"$(pwd)" env PATH=3D"${toolch=
ain}:${PATH}" \
                                'make ARCH=3D$ARCHITECTURE CROSS_COMPILE=3D=
"$CCACHE $KERNEL_COMPILER" olddefconfig'
                fi
        else
                eval CCACHE_BASEDIR=3D"$(pwd)" env PATH=3D"${toolchain}:${P=
ATH}" \
                        'make $CTHREADS ARCH=3D$ARCHITECTURE CROSS_COMPILE=
=3D"$CCACHE $KERNEL_COMPILER" oldconfig'
                eval CCACHE_BASEDIR=3D"$(pwd)" env PATH=3D"${toolchain}:${P=
ATH}" \
                        'make $CTHREADS ARCH=3D$ARCHITECTURE CROSS_COMPILE=
=3D"$CCACHE $KERNEL_COMPILER" ${KERNEL_MENUCONFIG:-menuconfig}'

                [[ ${PIPESTATUS[0]} -ne 0 ]] &amp;&amp; exit_with_error "Er=
ror kernel menuconfig failed"

                # store kernel config in easily reachable place
                display_alert "Exporting new kernel config" "$DEST/config/$=
LINUXCONFIG.config" "info"
                cp .config "${DEST}/config/${LINUXCONFIG}.config"
                cp .config "${EXTER}/config/kernel/${LINUXCONFIG}.config"
                # export defconfig too if requested
                if [[ $KERNEL_EXPORT_DEFCONFIG =3D=3D yes ]]; then
                        eval CCACHE_BASEDIR=3D"$(pwd)" env PATH=3D"${toolch=
ain}:${PATH}" \
                                'make ARCH=3D$ARCHITECTURE CROSS_COMPILE=3D=
"$CCACHE $KERNEL_COMPILER" savedefconfig'
                        [[ -f defconfig ]] &amp;&amp; cp defconfig "${DEST}=
/config/${LINUXCONFIG}.defconfig"
                fi
        fi

        # create linux-source package - with already patched sources
        # We will build this package first and clear the memory.
        if [[ $BUILD_KSRC !=3D no ]]; then
                create_linux-source_package
        fi

        echo -e "\n\t=3D=3D kernel =3D=3D\n" &gt;&gt; "${DEST}"/${LOG_SUBPA=
TH}/compilation.log
        eval CCACHE_BASEDIR=3D"$(pwd)" env PATH=3D"${toolchain}:${PATH}" \
                'make $CTHREADS ARCH=3D$ARCHITECTURE \
                CROSS_COMPILE=3D"$CCACHE $KERNEL_COMPILER" \
                $SRC_LOADADDR \
                LOCALVERSION=3D"-$LINUXFAMILY" \
                $KERNEL_IMAGE_TYPE ${KERNEL_EXTRA_TARGETS:-modules dtbs} 2&=
gt;&gt;$DEST/${LOG_SUBPATH}/compilation.log' \
                ${PROGRESS_LOG_TO_FILE:+' | tee -a $DEST/${LOG_SUBPATH}/com=
pilation.log'} \
                ${OUTPUT_DIALOG:+' | dialog --backtitle "$backtitle" \
                --progressbox "Compiling kernel..." $TTY_Y $TTY_X'} \
                ${OUTPUT_VERYSILENT:+' &gt;/dev/null 2&gt;/dev/null'}

        if [[ ${PIPESTATUS[0]} -ne 0 || ! -f arch/$ARCHITECTURE/boot/$KERNE=
L_IMAGE_TYPE ]]; then
                grep -i error $DEST/${LOG_SUBPATH}/compilation.log
                exit_with_error "Kernel was not built" "@host"
        fi

        # different packaging for 4.3+
        if linux-version compare "${version}" ge 4.3; then
                local kernel_packing=3D"bindeb-pkg"
        else
                local kernel_packing=3D"deb-pkg"
        fi

        #if [[ $BRANCH =3D=3D legacy &amp;&amp; $LINUXFAMILY =3D~ sun50iw2|=
sun50iw6|sun50iw9 ]]; then
        #       make -C modules/gpu LICHEE_MOD_DIR=3D${SRC}/.tmp/gpu_module=
s_${LINUXFAMILY} LICHEE_KDIR=3D${kerneldir} CROSS_COMPILE=3D$toolchain/$KER=
NEL_COMPILER ARCH=3D$ARCHITECTURE
        #fi

        display_alert "Creating packages"

        # produce deb packages: image, headers, firmware, dtb
        echo -e "\n\t=3D=3D deb packages: image, headers, firmware, dtb =3D=
=3D\n" &gt;&gt; "${DEST}"/${LOG_SUBPATH}/compilation.log
        eval CCACHE_BASEDIR=3D"$(pwd)" env PATH=3D"${toolchain}:${PATH}" \
                'make $CTHREADS $kernel_packing \
                KDEB_PKGVERSION=3D$REVISION \
                KDEB_COMPRESS=3D${DEB_COMPRESS} \
                BRANCH=3D$BRANCH \
                LOCALVERSION=3D"-${LINUXFAMILY}" \
                KBUILD_DEBARCH=3D$ARCH \
                ARCH=3D$ARCHITECTURE \
                DEBFULLNAME=3D"$MAINTAINER" \
                DEBEMAIL=3D"$MAINTAINERMAIL" \
                CROSS_COMPILE=3D"$CCACHE $KERNEL_COMPILER" 2&gt;&gt;$DEST/$=
{LOG_SUBPATH}/compilation.log' \
                ${PROGRESS_LOG_TO_FILE:+' | tee -a $DEST/${LOG_SUBPATH}/com=
pilation.log'} \
                ${OUTPUT_DIALOG:+' | dialog --backtitle "$backtitle" --prog=
ressbox "Creating kernel packages..." $TTY_Y $TTY_X'} \
                ${OUTPUT_VERYSILENT:+' &gt;/dev/null 2&gt;/dev/null'}

        cd .. || exit
        # remove firmare image packages here - easier than patching ~40 pac=
kaging scripts at once
        rm -f linux-firmware-image-*.deb

        rsync --remove-source-files -rq ./*.deb "${DEB_STORAGE}/" || exit_w=
ith_error "Failed moving kernel DEBs"    =20
}
</code></pre>
