<pre class=3D"highlighter-hljs" highlighted=3D"true"><code class=3D"languag=
e-shell highlighter-hljs hljs">compile_firmware()
{
        display_alert "Merging and packaging linux firmware" "@host" "info"

        local firmwaretempdir plugin_dir

        firmwaretempdir=3D$(mktemp -d)
        chmod 700 ${firmwaretempdir}
        trap "ret=3D\$?; rm -rf \"${firmwaretempdir}\" ; exit \$ret" 0 1 2 =
3 15
        plugin_dir=3D"orangepi-firmware${FULL}"
        mkdir -p "${firmwaretempdir}/${plugin_dir}/lib/firmware"

        [[ $IGNORE_UPDATES !=3D yes ]] &amp;&amp; fetch_from_repo "https://=
github.com/orangepi-xunlong/firmware" "${EXTER}/cache/sources/orangepi-firm=
ware-git" "branch:master"
        if [[ -n $FULL ]]; then
                [[ $IGNORE_UPDATES !=3D yes ]] &amp;&amp; fetch_from_repo "=
$MAINLINE_FIRMWARE_SOURCE" "${EXTER}/cache/sources/linux-firmware-git" "bra=
nch:master"
                # cp : create hardlinks
                cp -af --reflink=3Dauto "${EXTER}"/cache/sources/linux-firm=
ware-git/* "${firmwaretempdir}/${plugin_dir}/lib/firmware/"
        fi
        # overlay our firmware
        # cp : create hardlinks
        cp -af --reflink=3Dauto "${EXTER}"/cache/sources/orangepi-firmware-=
git/* "${firmwaretempdir}/${plugin_dir}/lib/firmware/"

        # cleanup what's not needed for sure
        rm -rf "${firmwaretempdir}/${plugin_dir}"/lib/firmware/{amdgpu,amd-=
ucode,radeon,nvidia,matrox,.git}
        cd "${firmwaretempdir}/${plugin_dir}" || exit

        # set up control file
        mkdir -p DEBIAN
        cat &lt;&lt;-END &gt; DEBIAN/control
