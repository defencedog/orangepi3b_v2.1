                mkdir -p "${destination}"/etc/mpv/
                cp "${EXTER}"/packages/bsp/mpv/mpv_sunxi.conf "${destinatio=
n}"/etc/mpv/mpv.conf
                echo "export VDPAU_OSD=3D1" &gt; "${destination}"/etc/profi=
le.d/90-vdpau.sh
