<pre class=3D"highlighter-hljs" highlighted=3D"true"><code class=3D"languag=
e-shell highlighter-hljs hljs">write_uboot()
{
        local loop=3D$1 revision
        display_alert "Writing U-boot bootloader" "$loop" "info"
        TEMP_DIR=3D$(mktemp -d || exit 1)
        chmod 700 ${TEMP_DIR}
        revision=3D${REVISION}
        if [[ -n $UPSTREM_VER ]]; then
                revision=3D${UPSTREM_VER}
                dpkg -x "${DEB_STORAGE}/u-boot/linux-u-boot-${BOARD}-${BRAN=
CH}_${revision}_${ARCH}.deb" ${TEMP_DIR}/
        else
        		# =E8=A7=A3=E5=8E=8B&lt;SDK&gt;/debs/u-boot/linux-u-boot-current-=
orangepi3b_1.0.6_arm64.deb=E5=88=B0=E4=B8=B4=E6=97=B6=E7=9B=AE=E5=BD=95
                dpkg -x "${DEB_STORAGE}/u-boot/${CHOSEN_UBOOT}_${revision}_=
${ARCH}.deb" ${TEMP_DIR}/
        fi

        # source platform install to read $DIR=EF=BC=8C=E6=89=A7=E8=A1=8Cpl=
atform_install.sh=E9=80=9A=E8=BF=87dd=E5=91=BD=E4=BB=A4=E5=AE=9E=E7=8E=B0u-=
boot=E7=9A=84=E5=AE=89=E8=A3=85=E5=8A=9F=E8=83=BD
        source ${TEMP_DIR}/usr/lib/u-boot/platform_install.sh
        write_uboot_platform "${TEMP_DIR}${DIR}" "$loop"   # DIR=3D/usr/lib=
/linux-u-boot-current-orangepi3b_1.0.6_arm64
        [[ $? -ne 0 ]] &amp;&amp; exit_with_error "U-boot bootloader failed=
 to install" "@host"
        rm -rf ${TEMP_DIR}
}
</code></pre>
